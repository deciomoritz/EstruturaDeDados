#include "Lista.h"
#include <string>

class ListaChar: public Lista<char*> {

	bool igual(char* t1, char* t2) {
		std::string c1(t1);
		std::string c2(t2);
		return c1 == c2;
	}

	bool maior(char* t1, char* t2) {
		std::string c1(t1);
		std::string c2(t2);
		return c1 > c2;
	}

	bool menor(char* t1, char* t2) {
		std::string c1(t1);
		std::string c2(t2);
		return c1 < c2;
	}
};
