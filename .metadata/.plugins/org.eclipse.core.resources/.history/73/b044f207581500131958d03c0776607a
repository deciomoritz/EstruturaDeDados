#include "include/FachadaPilha.h"
#include "include/Lista.h"

#include "include/Lancamento.h"

#include <iostream>

using namespace std;

int main() {

	char c[7] = "batata";

	Lancamento l(c, 10);

	char d[7] = "abacaxi";

	Lancamento l2(d, 20);

	Lista<Lancamento> lista;

	lista.adicionar(l);
	lista.adicionar(2);


	cout << lista.remover().getNome() << endl;

	return 0;

}
