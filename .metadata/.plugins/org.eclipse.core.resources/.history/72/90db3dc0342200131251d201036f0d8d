#include "Lancamento.h"
#include "Lista.h"
#include <iostream>

using namespace std;

int main() {

	Lista<Lancamento*> l;

	Lista<Lancamento*> *l2 = &l;
	for (int i = 0; i < 4; i++) {
		Lancamento* t = new Lancamento("teste", i*3+4);
		l2->adicionarNoInicio(t);
	}

	for (int i = 0; i < 4; i++) {
		cout << l2->mostra(i)->getValor() << endl;
    }



	return 0;
}
