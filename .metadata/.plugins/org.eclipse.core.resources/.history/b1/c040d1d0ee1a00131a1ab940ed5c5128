#include "include/ListaChar.h"

bool ListaChar::igual(char* t1, char* t2) {
	std::string c1(t1);
	std::string c2(t2);
	return c1 == c2;
}

bool ListaChar::maior(char* t1, char* t2) {
	std::string c1(t1);
	std::string c2(t2);
	return c1 > c2;
}

bool ListaChar::menor(char* t1, char* t2) {
	std::string c1(t1);
	std::string c2(t2);
	return c1 < c2;
}
